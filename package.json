{
  "name": "@keeex/jssha",
  "version": "3.3.1",
  "description": "jsSHA implements the complete Secure Hash Standard (SHA) family (SHA-1, SHA-224/256/384/512, SHA3-224/256/384/512, SHAKE128/256, cSHAKE128/256, and KMAC128/256) with HMAC",
  "main": "./lib/sha.js",
  "cleanPublish": {
    "behavior": {
      "cleanSource": {
        "package.json": {
          "type": "json",
          "remove": [
            "repository",
            "cleanPublish"
          ],
          "add": {
            "homepage": "https://keeex.me/oss",
            "scripts": {
              "build": "tslib build",
              "check": "tslib check",
              "clean": "tslib clean",
              "prepack": "npm run clean && npm run build",
              "test": "tslib test"
            }
          }
        },
        "README.md": {
          "type": "text",
          "remove": [
            "https://devtools.keeex.me",
            "https://docs.keeex.me"
          ]
        },
        "sonar-project.properties": "remove"
      },
      "cleanPrepack": {
        "package.json": [
          {
            "type": "json",
            "add": {
              "homepage": "https://keeex.me/oss"
            }
          }
        ],
        "README.md": {
          "type": "text",
          "remove": [
            "https://devtools.keeex.me",
            "https://docs.keeex.me"
          ]
        }
      },
      "cleanPublish": {
        "package.json": [
          {
            "type": "json",
            "package": true,
            "remove": [
              "repository"
            ]
          }
        ]
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KeeeX/jsSha.git"
  },
  "keywords": [
    "SHA-1",
    "SHA-224",
    "SHA3-224",
    "SHA-256",
    "SHA3-256",
    "SHA-384",
    "SHA3-384",
    "SHA-512",
    "SHA3-512",
    "SHAKE-128",
    "SHAKE-256",
    "SHAKE128",
    "SHAKE256",
    "cSHAKE128",
    "cSHAKE256",
    "KMAC128",
    "KMAC256",
    "SHA1",
    "SHA224",
    "SHA256",
    "SHA384",
    "SHA512",
    "SHA-2",
    "SHA2",
    "SHA-3",
    "SHA3",
    "SHAKE",
    "cSHAKE",
    "cSHAKE-128",
    "cSHAKE-256",
    "HMAC",
    "KMAC",
    "KMAC-128",
    "KMAC-256",
    "hash",
    "cryptography"
  ],
  "license": "BSD-3-Clause",
  "author": "Brian Turek <brian.turek@gmail.com>",
  "engines": {
    "node": ">14"
  },
  "homepage": "https://keeex.me/oss",
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/preset-env": "^7.24.8",
    "@keeex/clean-publish": "^1.1.1",
    "@keeex/eslint-config": "^1.12.5",
    "@keeex/tslib": "^1.21.3",
    "@swc/core": "^1.7.0",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/rewire": "^2.5.28",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "babel-plugin-import-redirect": "^1.1.1",
    "c8": "^10.1.2",
    "chai": "^4.3.7",
    "eslint": "^8.46.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-i": "^2.29.1",
    "eslint-plugin-mocha": "^10.4.3",
    "eslint-plugin-promise": "^6.4.0",
    "eslint-plugin-tsdoc": "^0.3.0",
    "knip": "^5.26.0",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.5.3"
  },
  "files": [
    "lib",
    "web"
  ],
  "scripts": {
    "build": "tslib build",
    "check": "tslib check",
    "clean": "tslib clean",
    "postpack": "clean-publish restore",
    "prepack": "npm run clean && npm run build && clean-publish cleanPublish",
    "prepublishOnly": "publish_check.sh",
    "rawtest": "tslib rawtest --inspect --timeout 150000000",
    "sonar": "npm run clean && tslib reporting && sonar-scanner -Dsonar.projectVersion=$(jq .version package.json)",
    "test": "tslib test"
  },
  "tslib": {
    "buildWeb": true,
    "knip": {
      "entry": [
        "sha.ts",
        "sha1.ts",
        "sha256.ts",
        "sha512.ts",
        "sha3.ts"
      ]
    },
    "typedoc": false
  },
  "type": "module"
}
